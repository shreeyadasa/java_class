import java.util.*;
class prime{
public static void main(String args[])
{
int num,i,flag=1;
System.out.println("Enter the number");
Scanner sc= new Scanner(System.in);
num=sc.nextInt();
for(i=2;i<num;i++)
{
if(num%i==0){
flag=0;
break;
}
}
if(flag==0)
System.out.println(num+" is not prime");
else
System.out.println(num+" is prime");
}}
